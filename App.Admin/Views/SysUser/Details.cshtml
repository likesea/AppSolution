@model App.Models.Sys.SysUserModel
@using App.Common;
@using App.Admin;
@using App.Models.Sys;
@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Index_LayoutEdit.cshtml";

}
<script type="text/javascript">
    $(function () {
        $("#btnReturn").click(function () {
            window.parent.frameReturnByClose();
        });
    });
</script>
<div class="mvctool bgb">
    <a id="btnReturn" style="float: left;" class="l-btn l-btn-plain"><span class="l-btn-left"><span class="l-btn-text icon-return" style="padding-left: 20px;">返回</span></span></a>
</div>
@using (Html.BeginForm())
{
    <table class="form_table setinput355">
        <tbody>
            <tr>
                <th>
                    @Html.LabelFor(model => model.CreateTime)
                </th>
                <td>
                    @Html.EditorFor(model => model.CreateTime)
                </td>
            </tr>
            <tr>
                <th>
                    @Html.LabelFor(model => model.CreatePerson)
                </th>
                <td>
                    @Html.EditorFor(model => model.CreatePerson)
                </td>
            </tr>
            <tr>
                <th>
                    @Html.LabelFor(model => model.UserName)
                </th>
                <td>
                    @Html.EditorFor(model => model.UserName)
                </td>
            </tr>
            <tr>
                <th>
                    @Html.LabelFor(model => model.School)
                </th>
                <td>
                    @Html.EditorFor(model => model.School)
                </td>
            </tr>
            <tr>
                <th>
                    @Html.LabelFor(model => model.PhoneNumber)
                </th>
                <td>
                    @Html.EditorFor(model => model.PhoneNumber)
                </td>
            </tr>

        </tbody>
    </table>
}
